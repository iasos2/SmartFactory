namespace StringPrint
{
    internal class Program
    {
        static void Main(string[] args)
        {
            // 3이라는 변수와 4라는 변수를 선언하고 3 + 5를 계산한다.

            // 3 + 5를 변수를 이용하여 결과를 출력해보세요.
            int value1 = 3; // = 대입연산자 R -> L
            int value2 = 5;

            int result = value1 + value2;

            Console.WriteLine(result);
        }
    }
}
